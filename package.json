{
  "name": "osm-ui-react",
  "version": "0.2.0",
  "homepage": "https://osm-ui.github.io/react",
  "repository": "https://github.com/osm-ui/react",
  "author": "Guillaume AMAT <guillaume@amat.io>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook -o docs",
    "build-dist": "cross-env NODE_ENV=production webpack --progress",
    "build": "npm run build-dist && npm run build-storybook",
    "precommit": "lint-staged"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,less}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{js,jsx,json}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-info": "^3.2.12",
    "@storybook/addon-knobs": "^3.2.12",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "leaflet": "^1.2.0",
    "leaflet.locatecontrol": "^0.62.0",
    "polished": "^1.7.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-fontawesome": "^1.6.1",
    "react-leaflet": "^1.7.0",
    "react-lorem-component": "^0.12.2",
    "react-placeholder": "^1.0.6",
