    "husky": "^0.14.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "raw-loader": "^0.5.1",
    "storybook-host": "^4.1.4",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  }
